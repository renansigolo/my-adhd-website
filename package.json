{
  "name": "my-adhd-website",
  "version": "3.9.2",
  "description": "My ADHD Website",
  "homepage": "https://myadhd.com.au",
  "bugs": {
    "url": "https://github.com/renansigolo/my-adhd-website/issues",
    "email": "renan.sigolo@gmail.com"
  },
  "license": "Apache-2.0",
  "author": "Renan Sigolo <renan.sigolo@gmail.com>",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/renansigolo/my-adhd-website.git"
  },
  "scripts": {
    "build": "next build",
    "check:format": "prettier --check .",
    "clean": "rm -rf package-lock.json node_modules .next",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "next dev --turbopack",
    "format": "prettier '**/*' --write --ignore-unknown",
    "lint:fix": "npm run lint -- --fix",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "postversion": "git push && git push --tags",
    "prepare": "husky",
    "start": "next start",
    "svgo": "npx svgo --multipass -f public -r",
    "test": "npm run build && concurrently -k npm:start npm:cy:run",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-brands-svg-icons": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@headlessui/react": "2.2.0",
    "@heroicons/react": "2.2.0",
    "@tailwindcss/forms": "0.5.10",
    "@vercel/analytics": "1.4.1",
    "@vercel/speed-insights": "1.1.0",
    "firebase": "11.2.0",
    "next": "15.1.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-firebase-hooks": "5.1.1",
    "react-hook-form": "7.54.2",
    "react-hot-toast": "2.5.1",
    "react-intl": "7.1.5",
    "sharp": "0.33.5"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@tailwindcss/postcss": "4.0.3",
    "@types/gtag.js": "0.0.20",
    "chance": "1.1.12",
    "concurrently": "9.1.2",
    "cypress": "14.0.1",
    "dotenv": "16.4.7",
    "eslint": "8.57.1",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "10.0.1",
    "husky": "9.1.7",
    "next-sitemap": "4.2.3",
    "postcss": "8.5.1",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.6.11",
    "semantic-release": "24.2.1",
    "tailwindcss": "4.0.3"
  },
  "engines": {
    "node": "^20.11.1 || ^22.12.0",
    "npm": ">=10.2.4"
  }
}
