{
  "name": "my-adhd-website",
  "version": "3.9.2",
  "description": "My ADHD Website",
  "homepage": "https://myadhd.com.au",
  "type": "module",
  "bugs": {
    "url": "https://github.com/renansigolo/my-adhd-website/issues",
    "email": "renan.sigolo@gmail.com"
  },
  "license": "Apache-2.0",
  "author": "Renan Sigolo <renan.sigolo@gmail.com>",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/renansigolo/my-adhd-website.git"
  },
  "scripts": {
    "build": "next build",
    "check:format": "prettier --check .",
    "clean": "rm -rf package-lock.json node_modules .next",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "next dev --turbopack",
    "format": "prettier '**/*' --write --ignore-unknown",
    "lint:fix": "npm run lint -- --fix",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "postversion": "git push && git push --tags",
    "prepare": "husky",
    "start": "next start",
    "svgo": "npx svgo --multipass -f public -r",
    "test": "npm run build && concurrently -k npm:start npm:cy:run",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@aws-sdk/client-sesv2": "^3.844.0",
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-brands-svg-icons": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@headlessui/react": "2.2.4",
    "@heroicons/react": "2.2.0",
    "@tailwindcss/forms": "0.5.10",
    "@vercel/analytics": "1.5.0",
    "@vercel/speed-insights": "1.2.0",
    "firebase": "11.10.0",
    "next": "15.4.0",
    "next-intl": "^4.3.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-firebase-hooks": "5.1.1",
    "react-hook-form": "7.60.0",
    "react-hot-toast": "2.5.2",
    "sharp": "0.34.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/eslintrc": "3.3.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@tailwindcss/postcss": "4.1.11",
    "@types/gtag.js": "0.0.20",
    "@types/node": "22.16.0",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "chance": "1.1.13",
    "concurrently": "9.2.0",
    "cypress": "14.5.1",
    "dotenv": "17.1.0",
    "eslint": "9.30.1",
    "eslint-config-next": "15.3.5",
    "eslint-config-prettier": "10.1.5",
    "husky": "9.1.7",
    "next-sitemap": "4.2.3",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.13",
    "semantic-release": "24.2.6",
    "tailwindcss": "4.1.11",
    "typescript": "5.8.3"
  }
}
