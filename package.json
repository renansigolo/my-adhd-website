{
  "name": "my-adhd-website",
  "version": "3.10.4",
  "description": "My ADHD Website",
  "homepage": "https://myadhd.com.au",
  "type": "module",
  "bugs": {
    "url": "https://github.com/renansigolo/my-adhd-website/issues",
    "email": "renan.sigolo@gmail.com"
  },
  "license": "Apache-2.0",
  "author": "Renan Sigolo <renan.sigolo@gmail.com>",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/renansigolo/my-adhd-website.git"
  },
  "scripts": {
    "build": "next build",
    "check:format": "prettier --check .",
    "clean": "rm -rf package-lock.json node_modules .next",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "next dev",
    "format": "prettier '**/*' --write --ignore-unknown",
    "lint:fix": "npm run lint -- --fix",
    "lint": "eslint",
    "postbuild": "next-sitemap",
    "postversion": "git push && git push --tags",
    "prepare": "husky",
    "start": "next start",
    "svgo": "npx svgo --multipass -f public -r",
    "test": "npm run build && concurrently -k npm:start npm:cy:run",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@aws-sdk/client-sesv2": "3.943.0",
    "@fortawesome/fontawesome-svg-core": "7.1.0",
    "@fortawesome/free-brands-svg-icons": "7.1.0",
    "@fortawesome/free-solid-svg-icons": "7.1.0",
    "@fortawesome/react-fontawesome": "3.1.1",
    "@headlessui/react": "2.2.9",
    "@heroicons/react": "2.2.0",
    "@hookform/resolvers": "5.2.2",
    "@tailwindcss/forms": "0.5.10",
    "@vercel/analytics": "1.6.1",
    "next": "16.0.7",
    "next-intl": "4.5.8",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "7.68.0",
    "react-hot-toast": "2.6.0",
    "sharp": "0.34.5",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@tailwindcss/postcss": "4.1.17",
    "@tailwindcss/typography": "0.5.19",
    "@types/gtag.js": "0.0.20",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "babel-plugin-react-compiler": "1.0.0",
    "chance": "1.1.13",
    "concurrently": "9.2.1",
    "cypress": "15.7.1",
    "eslint": "9.39.1",
    "eslint-config-next": "16.0.7",
    "eslint-config-prettier": "10.1.8",
    "husky": "9.1.7",
    "next-sitemap": "4.2.3",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "prettier-plugin-tailwindcss": "0.7.2",
    "semantic-release": "25.0.2",
    "tailwindcss": "4.1.17",
    "typescript": "5.9.3"
  },
  "overrides": {
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  }
}
