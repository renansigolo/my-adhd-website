{
  "name": "my-adhd-website",
  "version": "3.3.0",
  "description": "My ADHD Website",
  "homepage": "https://myadhd.com.au",
  "bugs": {
    "url": "https://github.com/renansigolo/my-adhd-website/issues",
    "email": "renan.sigolo@gmail.com"
  },
  "license": "Apache-2.0",
  "author": "Renan Sigolo <renan.sigolo@gmail.com>",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/renansigolo/my-adhd-website.git"
  },
  "scripts": {
    "build": "next build",
    "check": "prettier --check .",
    "clean": "rm -rf package-lock.json node_modules .next",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "next dev",
    "format": "prettier '**/*' --write --ignore-unknown",
    "lint:fix": "npm run lint -- --fix .",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "postversion": "git push && git push --tags",
    "prepare": "husky install",
    "start": "next start",
    "test": "npm run build && concurrently -k npm:start npm:cy:run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "@headlessui/react": "1.4.2",
    "@heroicons/react": "1.0.5",
    "@tailwindcss/aspect-ratio": "0.3.0",
    "@tailwindcss/forms": "0.3.4",
    "@tailwindcss/typography": "0.4.1",
    "axios": "0.24.0",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.21.0",
    "react-hot-toast": "2.1.1",
    "react-intl": "5.23.0",
    "sharp": "0.29.3"
  },
  "devDependencies": {
    "autoprefixer": "10.4.0",
    "concurrently": "6.4.0",
    "cypress": "9.1.1",
    "dotenv": "10.0.0",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "8.3.0",
    "faker": "5.5.3",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "next-sitemap": "1.6.203",
    "postcss": "8.4.4",
    "prettier": "2.5.1",
    "tailwindcss": "2.2.19"
  },
  "engines": {
    "node": "^14.15.0 || >=16.13.0",
    "npm": ">=6.14.8",
    "yarn": "NO LONGER USED - Please use npm"
  },
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Firefox version",
    "last 2 Edge major versions",
    "last 2 Safari major versions",
    "last 2 iOS major versions",
    "Firefox ESR",
    "not IE 11"
  ],
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
