{
  "name": "my-adhd-website",
  "version": "3.9.1",
  "description": "My ADHD Website",
  "homepage": "https://myadhd.com.au",
  "bugs": {
    "url": "https://github.com/renansigolo/my-adhd-website/issues",
    "email": "renan.sigolo@gmail.com"
  },
  "license": "Apache-2.0",
  "author": "Renan Sigolo <renan.sigolo@gmail.com>",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/renansigolo/my-adhd-website.git"
  },
  "scripts": {
    "build": "next build",
    "check:format": "prettier --check .",
    "clean": "rm -rf package-lock.json node_modules .next",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "next dev",
    "format": "prettier '**/*' --write --ignore-unknown",
    "lint:fix": "npm run lint -- --fix",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "postversion": "git push && git push --tags",
    "prepare": "husky",
    "start": "next start",
    "svgo": "npx svgo --multipass -f public -r",
    "test": "npm run build && concurrently -k npm:start npm:cy:run",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "@fortawesome/free-brands-svg-icons": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@headlessui/react": "1.7.18",
    "@heroicons/react": "2.1.1",
    "@tailwindcss/forms": "0.5.7",
    "@vercel/analytics": "1.2.2",
    "@vercel/speed-insights": "1.0.10",
    "firebase": "10.8.1",
    "next": "14.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-firebase-hooks": "5.1.1",
    "react-hook-form": "7.51.0",
    "react-hot-toast": "2.4.1",
    "react-intl": "6.6.2",
    "sharp": "0.33.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.0.3",
    "@commitlint/config-conventional": "19.0.3",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/gtag.js": "0.0.19",
    "autoprefixer": "10.4.18",
    "chance": "1.1.11",
    "concurrently": "8.2.2",
    "cypress": "13.6.6",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.1",
    "eslint-config-prettier": "9.1.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "next-sitemap": "4.2.3",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.11",
    "semantic-release": "23.0.2",
    "tailwindcss": "3.4.1"
  },
  "engines": {
    "node": "^18.17.0 || ^20.9.0",
    "npm": ">=9.6.7"
  },
  "browserslist": [
    "last 2 Chrome version",
    "last 1 Firefox version",
    "last 2 Edge major versions",
    "last 2 Safari major versions",
    "last 2 iOS major versions",
    "Firefox ESR"
  ],
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  }
}
