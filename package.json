{
  "name": "my-adhd-website",
  "version": "3.3.0",
  "description": "My ADHD Website",
  "homepage": "https://myadhd.com.au",
  "bugs": {
    "url": "https://github.com/renansigolo/my-adhd-website/issues",
    "email": "renan.sigolo@gmail.com"
  },
  "license": "Apache-2.0",
  "author": "Renan Sigolo <renan.sigolo@gmail.com>",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/renansigolo/my-adhd-website.git"
  },
  "scripts": {
    "build": "next build",
    "check:format": "prettier --check .",
    "clean": "rm -rf package-lock.json node_modules .next",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "next dev",
    "format": "prettier '**/*' --write --ignore-unknown",
    "lint:fix": "npm run lint -- --fix",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "postversion": "git push && git push --tags",
    "prepare": "husky install",
    "start": "next start",
    "svgo": "npx svgo --multipass -f public -r",
    "test": "npm run build && concurrently -k npm:start npm:cy:run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.1.2",
    "@fortawesome/free-brands-svg-icons": "6.1.2",
    "@fortawesome/free-solid-svg-icons": "6.1.2",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@headlessui/react": "1.6.6",
    "@heroicons/react": "2.0.8",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/typography": "0.5.4",
    "axios": "0.27.2",
    "firebase": "9.9.3",
    "next": "12.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-firebase-hooks": "5.0.3",
    "react-hook-form": "7.34.2",
    "react-hot-toast": "2.3.0",
    "react-intl": "6.1.0",
    "sharp": "0.30.7"
  },
  "devDependencies": {
    "@types/gtag.js": "0.0.10",
    "autoprefixer": "10.4.8",
    "chance": "1.1.8",
    "concurrently": "7.3.0",
    "cypress": "10.6.0",
    "dotenv": "16.0.1",
    "eslint": "8.23.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "next-sitemap": "3.1.21",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "tailwindcss": "3.0.8"
  },
  "engines": {
    "node": "^16.13.0 || ^18.0.0",
    "npm": ">=8.1.0",
    "yarn": "NO LONGER USED - Please use npm"
  },
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Firefox version",
    "last 2 Edge major versions",
    "last 2 Safari major versions",
    "last 2 iOS major versions",
    "Firefox ESR",
    "not IE 11"
  ],
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  }
}
