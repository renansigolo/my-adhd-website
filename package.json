{
  "name": "my-adhd-website",
  "version": "3.3.0",
  "description": "My ADHD Website",
  "homepage": "https://myadhd.com.au",
  "bugs": {
    "url": "https://github.com/renansigolo/my-adhd-website/issues",
    "email": "renan.sigolo@gmail.com"
  },
  "license": "Apache-2.0",
  "author": "Renan Sigolo <renan.sigolo@gmail.com>",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/renansigolo/my-adhd-website.git"
  },
  "scripts": {
    "build": "next build",
    "check:format": "prettier --check .",
    "clean": "rm -rf package-lock.json node_modules .next",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "next dev",
    "format": "prettier '**/*' --write --ignore-unknown",
    "lint:fix": "npm run lint -- --fix",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "postversion": "git push && git push --tags",
    "prepare": "husky install",
    "start": "next start",
    "svgo": "npx svgo --multipass -f public/images -r",
    "test": "npm run build && concurrently -k npm:start npm:cy:run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-brands-svg-icons": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@fortawesome/react-fontawesome": "0.1.18",
    "@headlessui/react": "1.6.4",
    "@heroicons/react": "1.0.6",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/typography": "0.5.2",
    "axios": "0.27.2",
    "firebase": "9.8.2",
    "next": "12.1.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.31.3",
    "react-hot-toast": "2.2.0",
    "react-intl": "6.0.4",
    "sharp": "0.30.6"
  },
  "devDependencies": {
    "autoprefixer": "10.4.7",
    "chance": "1.1.8",
    "concurrently": "7.2.1",
    "cypress": "10.0.3",
    "dotenv": "16.0.1",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.30.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.0",
    "next-sitemap": "3.0.5",
    "postcss": "8.4.14",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "0.1.11",
    "tailwindcss": "3.0.8"
  },
  "engines": {
    "node": "^14.15.0 || >=16.13.0",
    "npm": ">=6.14.8",
    "yarn": "NO LONGER USED - Please use npm"
  },
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Firefox version",
    "last 2 Edge major versions",
    "last 2 Safari major versions",
    "last 2 iOS major versions",
    "Firefox ESR",
    "not IE 11"
  ],
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  }
}
