{
  "name": "my-adhd-website",
  "version": "3.8.0",
  "description": "My ADHD Website",
  "homepage": "https://myadhd.com.au",
  "bugs": {
    "url": "https://github.com/renansigolo/my-adhd-website/issues",
    "email": "renan.sigolo@gmail.com"
  },
  "license": "Apache-2.0",
  "author": "Renan Sigolo <renan.sigolo@gmail.com>",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/renansigolo/my-adhd-website.git"
  },
  "scripts": {
    "build": "next build",
    "check:format": "prettier --check .",
    "clean": "rm -rf package-lock.json node_modules .next",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "next dev",
    "format": "prettier '**/*' --write --ignore-unknown",
    "lint:fix": "npm run lint -- --fix",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "postversion": "git push && git push --tags",
    "prepare": "husky install",
    "start": "next start",
    "svgo": "npx svgo --multipass -f public -r",
    "test": "npm run build && concurrently -k npm:start npm:cy:run",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.4.2",
    "@fortawesome/free-brands-svg-icons": "6.4.2",
    "@fortawesome/free-solid-svg-icons": "6.4.2",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@headlessui/react": "1.7.17",
    "@heroicons/react": "2.0.18",
    "@tailwindcss/forms": "0.5.6",
    "@vercel/analytics": "1.1.1",
    "axios": "1.6.0",
    "firebase": "10.5.2",
    "next": "13.5.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-firebase-hooks": "5.1.1",
    "react-hook-form": "7.48.1",
    "react-hot-toast": "2.4.1",
    "react-intl": "6.5.1",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@commitlint/cli": "18.2.0",
    "@commitlint/config-conventional": "18.1.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/gtag.js": "0.0.17",
    "autoprefixer": "10.4.16",
    "chance": "1.1.11",
    "concurrently": "8.2.2",
    "cypress": "13.4.0",
    "dotenv": "16.3.1",
    "eslint": "8.53.0",
    "eslint-config-next": "13.5.6",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "lint-staged": "15.0.2",
    "next-sitemap": "4.2.3",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.6",
    "semantic-release": "22.0.7",
    "tailwindcss": "3.3.5"
  },
  "engines": {
    "node": "^18.12.0 || ^20.0.0",
    "npm": ">=8.1.0",
    "yarn": "NO LONGER USED - Please use npm"
  },
  "browserslist": [
    "last 1 Chrome version",
    "last 1 Firefox version",
    "last 2 Edge major versions",
    "last 2 Safari major versions",
    "last 2 iOS major versions",
    "Firefox ESR",
    "not IE 11"
  ],
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  }
}
