{
  // "extends": ["plugin:react/recommended", "next/core-web-vitals", "prettier"],
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "no-restricted-imports": [
      "error",
      {
        "patterns": ["..*", "src"]
      }
    ],
    "react/jsx-sort-props": [
      1,
      {
        "reservedFirst": true,
        "shorthandFirst": true,
        "callbacksLast": true,
        "multiline": "last"
      }
    ]
  }
}
